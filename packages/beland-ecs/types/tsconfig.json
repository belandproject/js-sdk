{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "moduleResolution": "classic",
    "pretty": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "keyofStringsOnly": true,
    "inlineSourceMap": true,
    "sourceMap": false,
    "removeComments": false,
    "inlineSources": true,
    "useDefineForClassFields": false,
    "noLib": true,
    "allowJs": true,
    "types": [
      "env",
      "bld",
      "@beland/EthereumController",
      "@beland/SocialController",
      "@beland/Identity",
      "@beland/web3-provider",
      "@beland/EnvironmentAPI",
      "@beland/ParcelIdentity",
      "@beland/PortableExperiences",
      "@beland/RestrictedActions",
      "@beland/SignedFetch",
      "@beland/Players"
    ],
    "typeRoots": [
      "."
    ]
  }
}
